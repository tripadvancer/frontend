    },
    "dependencies": {
        "@types/node": "20.6.2",
        "@types/react": "18.2.22",
        "@types/react-dom": "18.2.7",
        "eslint": "8.49.0",
        "eslint-config-next": "13.4.19",
        "next": "13.4.19",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "typescript": "5.2.2",
        "yet-another-react-lightbox": "^3.12.2"
    },
    "devDependencies": {
        "@cloudflare/next-on-pages": "^1.6.1",
        "autoprefixer": "^10.4.15",
        "eslint-plugin-next-on-pages": "^1.6.1",
        "postcss": "^8.4.30",
        "prettier": "^3.0.3",
        "prettier-plugin-tailwindcss": "^0.5.4",
        "tailwindcss": "^3.3.3",
        "vercel": "^32.2.5"
